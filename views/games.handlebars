<table class="table table-striped table-light" id="table">
    <thead>
    </thead>
    <tbody id="tbody">
        <tr>
            <th>
                Games
            </th>
            <th>
                Genre
            </th>
            <th>
                Players
            </th>
        </tr>
        <tr>
            <td id="t1"></td>
            <td id="t2"></td>
            <td id="t3"></td>
        </tr>
    </tbody>
</table>


<script>
        $(document).ready(function () {
            console.log("ready!");

            $.ajax({
                type: 'GET',
                url: '/send_data',
                dataType: 'json',
            }).done(function (data) {
                console.log(data);

                for (var i = 0; i < data.Games.length; i++) {
                    $("#t1").append($(
                        '<tr><td>' + data.Games[i] + '</td></tr>'));
                }
                for (var j = 0; j < data.Games.length; j++) {
                    $("#t2").append($(
                        '<tr><td>' + data.Genre[j] + '</td></tr>'));
                }
                for (var c = 0; c < data.Games.length; c++) {
                    $("#t3").append($(
                        '<tr><td>' + data.Players[c] + '</td></tr>'));
                }
            });
        });            
</script>