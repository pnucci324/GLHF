<table class="table table-striped table-light">
    <thead>
    </thead>
    <tbody id="tbody">
    </tbody>
</table>


<script>
    $(document).ready(function () {
        console.log("ready!");
        $.ajax({
            type: 'GET',
            url: '/send_data',
            //    contentType: 'application/json; charset=utf-8',
            dataType: 'json',
        }).done(function (data) {
            // table_data = JSON.stringify(data);
            console.log(data);


            Object.keys(data).forEach(function (key, values) {
                table_data = JSON.parse(JSON.stringify(data[key]));
                console.log(Object.values(table_data));

                for (var i = 0; i < table_data.length; i++) {
                    $("#tbody").append($(
                        '<tr><td>' + table_data[i] + '</td></tr>'));
                }
            });
        });
    });
</script>